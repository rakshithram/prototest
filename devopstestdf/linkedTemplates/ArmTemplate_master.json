{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "devopstestdf"
		},
		"srclinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'srclinkedservice'"
		},
		"tgtlinkedservice_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'tgtlinkedservice'"
		},
		"srcblob_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "AccessBotOriginal.txt"
		},
		"srcblob_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "devrepo"
		},
		"tgtblob_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": ""
		},
		"tgtblob_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "qarepo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"srclinkedservice_connectionString": {
						"value": "[parameters('srclinkedservice_connectionString')]"
					},
					"tgtlinkedservice_connectionString": {
						"value": "[parameters('tgtlinkedservice_connectionString')]"
					},
					"srcblob_properties_typeProperties_fileName": {
						"value": "[parameters('srcblob_properties_typeProperties_fileName')]"
					},
					"srcblob_properties_typeProperties_folderPath": {
						"value": "[parameters('srcblob_properties_typeProperties_folderPath')]"
					},
					"tgtblob_properties_typeProperties_fileName": {
						"value": "[parameters('tgtblob_properties_typeProperties_fileName')]"
					},
					"tgtblob_properties_typeProperties_folderPath": {
						"value": "[parameters('tgtblob_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}